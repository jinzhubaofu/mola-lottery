{
    "name": "mola-lottery",
    "version": "1.0.8",
    "description": "A MOLA Lottery Component",
    "main": "lib/Lottery.js",
    "scripts": {
        "start": "NODE_ENV=dev node tools/server.js",
        "build": "npm run lint && rimraf lib && NODE_ENV=production gulp build",
        "lint": "fecs",
        "prepublish": "npm run lint && npm run build",
        "test": "NODE_ENV=test karma start ./test/karma.local.conf.js",
        "test-ci": "NODE_ENV=test karma start ./test/karma.travis.conf.js",
        "coveralls": "cat test/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
        "docs": "esdoc -c esdoc.json"
    },
    "keywords": [
        "mola"
    ],
    "authors": [
        "ludafa <ludafa@outlook.com>",
        "jingyuanZhang"
    ],
    "license": "MIT",
    "dependencies": {
        "inline-style-prefixer": "^2.0.4",
        "react": "^15.4.0",
        "jsonp-es6": "^1.0.0",
        "mola": "^1.0.8"
    },
    "devDependencies": {
        "gulp-stylus": "^2.6.0",
        "babel-istanbul": "^0.11.0",
        "babel-loader": "^6.2.5",
        "babel-plugin-external-helpers": "^6.8.0",
        "babel-plugin-external-helpers-insert-require": "^1.0.0",
        "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
        "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
        "babel-plugin-transform-es3-property-literals": "^6.8.0",
        "babel-plugin-transform-object-rest-spread": "^6.8.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-2": "^6.13.0",
        "esdoc": "^0.4.8",
        "esdoc-es7-plugin": "0.0.3",
        "babelify": "^7.3.0",
        "browserify": "^13.1.0",
        "browserify-istanbul": "^2.0.0",
        "coveralls": "^2.11.12",
        "css-loader": "^0.24.0",
        "fecs": "^0.8.6",
        "file-loader": "^0.9.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^6.1.2",
        "gulp-babel-external-helpers": "^2.0.2",
        "gulp-clean": "^0.3.2",
        "gulp-sourcemaps": "^1.6.0",
        "jasmine": "^2.4.1",
        "karma": "^1.2.0",
        "karma-babel-preprocessor": "^6.0.1",
        "karma-browserify": "^5.1.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-firefox-launcher": "^1.0.0",
        "karma-jasmine": "^1.0.2",
        "karma-sauce-launcher": "^1.0.0",
        "rimraf": "^2.5.4",
        "nib": "^1.1.2",
        "style-loader": "^0.13.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^2.3.1",
        "watchify": "^3.7.0",
        "webpack": "^1.13.2",
        "webpack-dev-server": "^1.15.0",
        "webpack-hot-middleware": "^2.12.2"
    },
    "babel": {
        "env": {
            "dev": {
                "presets": [
                    [
                        "es2015",
                        {
                            "loose": true
                        }
                    ],
                    "stage-2",
                    "react"
                ]
            },
            "production": {
                "presets": [
                    [
                        "es2015",
                        {
                            "loose": true,
                            "modules": "umd"
                        }
                    ],
                    "stage-2",
                    "react"
                ],
                "plugins": [
                    "transform-es3-property-literals",
                    "transform-es3-member-expression-literals"
                ]
            }
        }
    }
}
